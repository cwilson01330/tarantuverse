{
  "expo": {
    "name": "Tarantuverse",
    "slug": "tarantuverse",
    "version": "0.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "entryPoint": "./index.js",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tarantuverse.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Tarantuverse needs access to your photo library so you can select photos of your tarantulas to add to their profiles and track their growth over time. For example, you can upload a photo after a molt to document your tarantula's new appearance, or add photos to track enclosure setups.",
        "NSCameraUsageDescription": "Tarantuverse needs access to your camera so you can take photos of your tarantulas directly within the app to document molts, track growth, and maintain a visual record of your collection."
      },
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.tarantuverse.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "tarantuverse",
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.628209648097-qt596g5efb603ar3ml254qp1n0d3he1b"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1d412361-4866-482a-a0a6-5d525508d8b8"
      }
    }
  }
}