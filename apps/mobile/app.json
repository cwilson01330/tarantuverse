{
  "expo": {
    "name": "Tarantuverse",
    "slug": "tarantuverse",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tarantuverse.app"
    },
    "android": {
      "package": "com.tarantuverse.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "tarantuverse",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
