{
  "expo": {
    "name": "Tarantuverse",
    "slug": "tarantuverse",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tarantuverse.app"
    },
    "android": {
      "package": "com.tarantuverse.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "tarantuverse",
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1d412361-4866-482a-a0a6-5d525508d8b8"
      }
    }
  }
}
