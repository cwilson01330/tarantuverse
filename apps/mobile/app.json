{
  "expo": {
    "name": "Tarantuverse",
    "slug": "tarantuverse",
    "version": "0.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "entryPoint": "./index.js",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tarantuverse.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Tarantuverse needs access to your photo library to let you add photos to your tarantula profiles. For example, when your tarantula molts, you can select a photo from your library to document its new size and coloration. You can also add photos of enclosure setups, feeding responses, or other care-related images to track your collection over time.",
        "NSCameraUsageDescription": "Tarantuverse needs access to your camera so you can take photos of your tarantulas directly within the app to document molts, track growth, and maintain a visual record of your collection."
      },
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.tarantuverse.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "tarantuverse",
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.628209648097-qt596g5efb603ar3ml254qp1n0d3he1b"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-iap"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1d412361-4866-482a-a0a6-5d525508d8b8"
      }
    }
  }
}
