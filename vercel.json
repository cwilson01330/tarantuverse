{
  "version": 2,
  "buildCommand": "cd apps/web && pnpm install --no-frozen-lockfile && pnpm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install -g pnpm@latest",
  "framework": null,
  "routes": [
    {
      "src": "/404",
      "status": 404,
      "dest": "/404.html"
    },
    {
      "src": "/500",
      "status": 500,
      "dest": "/500.html"
    }
  ]
}
